	<div id="applicant-dialog" class="modal fade" style="display:none">
		<div class="modal-header">
			<a class="close" data-dismiss="modal">×</a>
			<h3>我要报名</h3>
		</div>
		<div class="modal-body" id="applicant-body">
			<form class="well form-horizontal" id="applicant-form" action="/home/applicant" method="POST">
				<p class="help-block" id="error-applicant"></p>
				<div class="control-group">
					<label class="control-label" for="applicant_username">用户名</label>
					<div class="controls">
						<input type="text" class="span3" id="applicant_username" name="applicant_username" placeholder="请输入用户名...">
					</div>
				</div>
				<div class="control-group">
					<label class="control-label" for="applicant_name">姓名</label>
					<div class="controls">
						<input type="text" class="span3" id="applicant_name" name="applicant_name" placeholder="请输入真实姓名...">
					</div>
				</div>
				<div class="control-group">
					<label class="control-label" for="applicant_phone">联系电话</label>
					<div class="controls">
						<input type="text" class="span3" id="applicant_phone" name="applicant_phone" placeholder="请输入联系电话...">
					</div>
				</div>
				<div class="control-group">
					<label class="control-label" for="applicant_email">邮箱</label>
					<div class="controls">
						<input type="text" class="span3" id="applicant_email" name="applicant_email" placeholder="请输入邮箱...">
					</div>
				</div>
				<div class="control-group">
					<label class="control-label" for="applicant_school">学校</label>
					<div class="controls">
						<div class="input-append">
							<input type="text" class="span3" id="applicant_school" name="applicant_school" readonly="readonly" placeholder="请选择学校...">
							<a class="btn" id="select-school" data-toggle="modal" href="#school-dialog" data-keyboard="true" data-backdrop="true">选择</a>
						</div>
					</div>
				</div>
				<div class="control-group">
					<label class="control-label" for="applicant_keywords">关键字</label>
					<div class="controls">
						<input type="text" class="span3" id="applicant_keywords" name="applicant_keywords" placeholder="请输入您的keywords...">
					</div>
				</div>
				<div class="control-group">
					<label class="control-label" for="applicant_comment">寄语</label>
					<div class="controls">
						<textarea id="applicant_comment" class="span3" name="applicant_comment" rows="3"></textarea>
					</div>
				</div>
				<input type="hidden" name="applicant_schoolid" id="applicant_schoolid">
			</form>
			<div class="modal-footer">
				<a href="#" id="submit-applicant" class="btn btn-warning">提交</a>
				<a class="btn" data-dismiss="modal">取消</a>
			</div>
		</div>
	</div>
